
<script src="https://ajax.microsoft.com/ajax/jquery/jquery-3.3.1.min.js" type="text/javascript"></script>
<link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css" />
<script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

@{
    ViewBag.Title = "Home Page";
}

<img id="logo" src="~/img/twnlogo.png" />

<div id="searchbarHolder">
    <div class="box">
        <form method="post" action="@Url.Action("Suggestion", "Suggestion")">
            @Html.TextBox("searchInput")

            <script>
        $("#searchInput").autocomplete({
    source: function (request, response) {
        $.ajax({
            url: '@Url.Action("GetSearchValue", "Home")',
            dataType: "json",
            data: { search: $("#searchInput").val() },
            success: function (data) {
                response($.map(data, function (item) {
                    return { label: item.Name, value: item.Name };
                }))
            }
        })
    },
    error: function (xhr, status, error) {
        alert("Error search");
    }
});
            </script>
            <button type="submit"><i class="fa fa-search"></i></button>
        </form>
    </div>

</div>

<div class="applogobox">
    <img id="applogo" src="~/img/getout_all_caps.png" />
</div>
